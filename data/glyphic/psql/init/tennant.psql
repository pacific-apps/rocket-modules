CREATE TABLE glyf_tennant (
    id INT(12) UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    privateId VARCHAR(32) NOT NULL,
    publicKey VARCHAR(32) NOT NULL,
    createdAt VARCHAR(32) NOT NULL,
    activatedAt VARCHAR(32) NOT NULL,
    group VARCHAR(32),
    pod VARCHAR(32),
    package VARCHAR(32),
    status VARCHAR(32) NOT NULL
);
