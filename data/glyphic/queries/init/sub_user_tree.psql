CREATE TABLE s_glyf_user_tree (
    id INT(12) UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    userId VARCHAR(32) NOT NULL,
    parentOfId VARCHAR(64) NOT NULL,
    siblingOfIds VARCHAR(64) NOT NULL,
    childOfId VARCHAR(32) NOT NULL,
    tennantId VARCHAR(32) NOT NULL,
    recordType VARCHAR(32) NOT NULL
);
